# EditorConfig configuration for the Core-Redirects Workspace, based on the config from the CoreMedia Blueprint Workspace
# https://editorconfig.org

root = false

[*]
charset = utf-8
continuation_indent_size = 8
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false
